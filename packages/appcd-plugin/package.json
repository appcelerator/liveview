  "description": "Titanium LiveView plugin for Appc Daemon",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc -b tsconfig.build.json",
    "dev": "tsc -b tsconfig.build.json --watch"
  },
  "author": "Axway Appcelerator",
  "maintainers": [
    "Jan Vennemann <jvennemann@axway.com>"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "@liveview/shared-utils": "^1.0.0",
    "fs-extra": "^9.0.1",
    "lodash.isequal": "^4.5.0",
    "node-titanium-sdk": "^5.1.1"

  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.2",
    "@types/lodash.isequal": "^4.5.5"
  },
  "appcd": {
    "name": "liveview",
    "type": "external",
